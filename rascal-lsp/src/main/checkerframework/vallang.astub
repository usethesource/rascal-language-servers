package io.usethesource.vallang;

import org.checkerframework.checker.nullness.qual.*;

public interface IWithKeywordParameters<T extends IValue> {

  @Nullable
  IValue getParameter(String label);

  //@EnsuresKeyForIf(result=true, expression="label", map="this")
  @EnsuresNonNullIf(expression="getParameter(#1)", result=true)
  boolean hasParameter(String label);
}
