begin 
    declare 
    input   : natural,  
    output  : natural,           
    repnr   : natural,
    rep     : natural,
    add(x: natural, y: natural): natural := x + y,
    multiply(x: natural, y: natural): natural := add(x, multiply(x - 1, y));
 
    input := 6;
    while input - 1 do 
        rep := output;
        repnr := input;
        while repnr - 1 do
            output := output + rep;
            repnr := repnr - 1
        od;
        input := input - 1
    od
end